@article{el_hypersonic_2004,
	title = {Hypersonic flow past slender bodies in dispersive hydrodynamics},
	volume = {333},
	abstract = {The problem of two-dimensional steady hypersonic ﬂow past a slender body is formulated for dispersive media. It is shown that for the hypersonic ﬂow, the original 2 + 0 boundary-value problem is asymptotically equivalent to the 1 + 1 piston problem for the fully nonlinear ﬂow in the same physical system, which allows one to take advantage of the analytic methods developed for one-dimensional systems. This type of equivalence, well known in ideal Euler gas dynamics, has not been established for dispersive hydrodynamics so far. Two examples pertaining to collisionless plasma dynamics are considered.},
