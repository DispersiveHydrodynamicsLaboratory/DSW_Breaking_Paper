@article{chung_spin_2014,
	title = {Spin transfer torque generated magnetic droplet solitons (invited)},
	volume = {115},
	issn = {0021-8979, 1089-7550},
	url = {http://scitation.aip.org/content/aip/journal/jap/115/17/10.1063/1.4870696},
	doi = {10.1063/1.4870696},
	abstract = {We present recent experimental and numerical advancements in the understanding of spin transfer torque generated magnetic droplet solitons. The experimental work focuses on nano-contact spin torque oscillators ({NC}-{STOs}) based on orthogonal (pseudo) spin valves where the Co fixed layer has an easy-plane anisotropy, and the [Co/Ni] free layer has a strong perpendicular magnetic anisotropy. The {NC}-{STO} resistance and microwave signal generation are measured simultaneously as a function of drive current and applied perpendicular magnetic field. Both exhibit dramatic transitions at a certain current dependent critical field value, where the microwave frequency drops 10 {GHz}, modulation sidebands appear, and the resistance exhibits a jump, while the magnetoresistance changes sign. We interpret these observations as the nucleation of a magnetic droplet soliton with a large fraction of its magnetization processing with an angle greater than 90°, i.e., around a direction opposite that of the applied field. This interpretation is corroborated by numerical simulations. When the field is further increased, we find that the droplet eventually collapses under the pressure from the Zeeman energy.},
	pages = {172612},
	number = {17},
	author = {Chung, S. and Mohseni, S. M. and Sani, S. R. and Iacocca, E. and Dumas, R. K. and Nguyen, T. N. Anh and Pogoryelov, Ye and Muduli, P. K. and Eklund, A. and Hoefer, M. and Åkerman, J.},
	urldate = {2014-04-16},
	year = {2014-04},
	keywords = {Fluid drops, magnetic anisotropy, magnons, Microwaves, Nucleation}
}

@article{conforti_resonant_2014,
	title = {Resonant radiation shed by dispersive shock waves},
	volume = {89},
	url = {http://link.aps.org/doi/10.1103/PhysRevA.89.013807},
	doi = {10.1103/PhysRevA.89.013807},
	abstract = {We analyze resonant radiation emitted by dispersive shock waves owing to higher-order dispersive corrections of the leading term in the defocusing nonlinear Schrödinger equation. We give criteria for calculating the radiated frequency based on an analytical estimate of the shock velocity and reveal a diversity of scenarios controllable via the corrections, ranging from the radiation-induced transition of the dispersive shock into a classical-type shock to the qualitative modification of the underlying gradient catastrophe or the competition between different breaking mechanisms.},
	pages = {013807},
	number = {1},
	author = {Conforti, Matteo and Baronio, Fabio and Trillo, Stefano},
	urldate = {2014-05-04},
	year = {2014}
}

