@inproceedings{mohseni_observation_2012,
	location = {Varberg, Sweden},
	title = {Observation of dissipative droplet soliton in nano-contact spin-torque oscillator with perpendicular magnetic anisotropy free layer},
	abstract = {Existing of a droplet soliton excitation in nano-contact spin-torque oscillators ({STOs}) with large perpendicular magnetic anisotropy ({PMA}) was predicted by Hoefer et al [Hoefer {PRB}] as a novel localized oscillating mode which has never seen yet in experiments. This mode is related to the dynamics of the “magnon droplet” pointing nearly opposite to its equilibrium direction. In this paper, we present the observation of novel localized dissipative droplet soliton mode in a nano-contact {STO}. Nano-contact diameter varies from 50 to 100 nm fabricated on an orthogonal spin valve with in-plane Co reference layer and {PMA} Co/Ni free layer separated by Cu, Co8/Cu/Co0.3[Ni0.8/Co0.4]x4, which thicknesses are given in nm. We have recently shown a high frequency operation at low field for this {STO} [{PSS} {RRL}], which confirm an existing of a large {PMA} free layer. We observed a linear dependent of {STO} frequency versus external field up to a moderate value, indicating small precession {STO} operation [{NIST} {PRB}]. Interestingly, at moderate field, there is an immediate drop, about 10 {GHz}, in frequency accompanying with a jump in resistance both observed in field and current sweeps. Observed signal has higher power than those with higher frequency mode. We show a relation between nano-contact size, droplet onset frequency, field and current and frequency drop which fit very well with theory. Our results are promising mechanism to get higher power of {STOs} and derive a new dynamics in nano-contact {STOs} which opens up many subjects for future studies.},
	author = {Mohseni, S. M. and Sani, S. R. and Pogoryelov, Ye. and Persson, J. and Chung, S. and Nguyen, T. N. Anh and Muduli, P. K. and Iacocca, E. and Bonetti, S. and Hoefer, M. A. and Deac, A. M. and Åkerman, J.},
	year = {2012-04}
}

